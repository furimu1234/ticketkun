{
  "name": "ticketkun",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "install": "pnpm -v",
    "build": "pnpm -F \"@ticket/*\" run build",
    "local": "pnpm build && pnpm start",
    "type-check": "pnpm -r --filter ./packages/* run type-check",
    "start": "pnpm -F \"@ticket/bot\" start",
    "dev": "pnpm -F \"@ticket/bot\" dev",
    "clean": "git clean -Xfd -e pnpm-lock.yaml",
    "generate": "pnpm --filter db run generate",
    "drizzle": "pnpm --filter db drizzle"
  },
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.43.1",
    "zod": "^4.3.5"
  }
}
